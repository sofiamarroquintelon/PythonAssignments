    "def fizzbuzz(n):\n",
    "        if (n %3 ==0) and (n %5 ==0):\n",
    "            print(f\"{n}: FizzBuzz\")\n",
    "        elif (n %3 ==0):\n",
    "            print(f\"{n}: Fizz\")\n",
    "        elif (n %5 == 0):\n",
    "            print(f\"{n}: Buzz\")"
   ]
